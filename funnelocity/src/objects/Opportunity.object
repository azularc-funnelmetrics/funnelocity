    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Opportunity_Close_Date</fullName>
        <active>true</active>
        <description>This rule fires if an opportunity is closed but the close date is before today</description>
        <errorConditionFormula>AND(  
         OR(
             AND(ISPICKVAL(StageName , &apos;Closed Won&apos;), NOT(ISPICKVAL(PRIORVALUE(StageName), &apos;Closed Won&apos;))), 
             AND(ISPICKVAL(StageName , &apos;Closed Lost&apos;), NOT(ISPICKVAL(PRIORVALUE(StageName) , &apos;Closed Lost&apos;)))
            ),
            
            CloseDate &lt;  TODAY() 
         )</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Please update the Close date as it is a past date</errorMessage>
    </validationRules>
</CustomObject>
